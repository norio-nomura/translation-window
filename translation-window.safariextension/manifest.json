{
    "name":"Translation Window",
    "version":"1.4",
    "description":"Ctrl+Alt+D activate Translation Window",
    "icons":{
        "16":"Icon-16.png",
        "48":"Icon-48.png",
        "128":"Icon-128.png"
    },
    "background_page":"global.html",
    "content_scripts":[
        {
            "matches":["https://*/*","http://*/*"],
            "js":["afterLoad.js"],
            "run_at":"document_end"
        }
    ],
    "options_page": "options.html",
    "permissions":["http://www.google.com/"]
}